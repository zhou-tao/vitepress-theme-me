{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "lib": ["esnext", "DOM"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vite/client"],
    "outDir": "dist"
  },
  "include": ["vue-shims.d.ts", "src/**/*.ts", "src/**/*.vue"],
  "exclude": ["example"]
}
